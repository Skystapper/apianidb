{
  "name": "anidb-udp-client",
  "version": "1.0.0",
  "description": "Anidb udp client module.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ci_install": "yarn install --immutable --immutable-cache --check-cache",
    "test": "jest --coverage",
    "verify": "yarn eslint . && yarn tsc && yarn test",
    "depcheck": "depcheck .",
    "build": "tsc --outDir ./dist",
    "docs": "typedoc src/index.ts --excludeExternals --out ./docs"
  },
  "jest": {
    "watchman": false,
    "modulePaths": [
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node16": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.1",
    "jest": "^29.0.3",
    "pino": "^8.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typedoc-plugin-versions": "^0.2.1",
    "typescript": "^4.8.3"
  }
}
